                                            end
                                        end
                                    end
                                end
                            elseif getgenv().list.Gun.UseGunMethod or getgenv().list.Sword.UseSwordMethod then
                                if getgenv().list.Melee.UseMelee then
                                    if getgenv().list.Melee.MeleeSkillZ then	
                                        l = getgenv().list.Melee.HoldZMelee
                                        down("Z")
