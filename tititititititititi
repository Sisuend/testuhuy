SE:AddToggle("Teleport Danger 6 ( Kitsune Island )",_G.PutFrozen,function(state)
if state then
_G.TpDangerSix = true
else
_G.TpDangerSix = false
StopTween(_G.TpDangerSix)
end

if _G.TpDangerSix then

for _,v in next, workspace.Boats[_G.Dangers]:GetDescendants() do
    if v.Name:find("VehicleSeat") then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
    end
end
end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.TpDangerSix then
                local player = game.Players.LocalPlayer
                local playerPos = player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character.HumanoidRootPart.Position

                if playerPos then
                    local maxRadius = 500
                    local closestVehicle = nil
                    local minDistance = math.huge

                    for _, boat in pairs(workspace.Boats[_G.Dangers]:GetChildren()) do
                        if boat:IsA("Model") and boat:FindFirstChild("VehicleSeat") then
                            local seatPosition = boat.VehicleSeat.Position
                            local distance = (playerPos - seatPosition).Magnitude

                            if distance < minDistance and distance <= maxRadius then
                                closestVehicle = boat
                                minDistance = distance
                            end
                        end
                    end

                    if closestVehicle then
                        local seatPosition = closestVehicle.VehicleSeat.Position
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(seatPosition)
                    end
                end
            end
        end)
    end
end)


spawn(function()
    while wait() do
        pcall(function()
            if _G.TpDangerSix then
                local batuLaut = {"SmallGroup", "SmallCluster", "MediumGroup", "MediumFlat", "Large", "Largest"}

                for _, v in pairs(workspace:GetChildren()) do
                    if table.find(batuLaut, v.Name) and v:IsA("Model") then
                        for _, part in pairs(v:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                            end
                        end
                    end
                end
            end
        end)
    end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.TpDangerSix then
                wait(3) 
                local model = workspace.Boats[_G.Dangers]
                if model then
                    local height = 100 -- Atur ketinggian yang diinginkan

                    local initialPosition = model.PrimaryPart.Position
                    local targetPosition = model.PrimaryPart.Position + Vector3.new(0, height, 0)

                    while (model.PrimaryPart.Position - targetPosition).Magnitude > 0.1 and _G.TpDangerSix do
                        local newY = initialPosition.Y + height
                        local newPosition = Vector3.new(model.PrimaryPart.Position.X, newY, model.PrimaryPart.Position.Z)
                        model:SetPrimaryPartCFrame(CFrame.new(newPosition))

                        task.wait()
                    end
                end
            end
        end)
    end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.TpDangerSix then
                wait(0.5) 
                local model = workspace.Boats[_G.Dangers]
                if model then
                    local speed = 10
                    local targetPosition = Vector3.new(-45051.46875, 56.1074333190918, 11439.3701171875)
                    local direction = (targetPosition - model.PrimaryPart.Position).unit
                    while (model.PrimaryPart.Position - targetPosition).Magnitude > 0.1 do
                        local BijiJahe = (model.PrimaryPart.Position - targetPosition).Magnitude
                        model:SetPrimaryPartCFrame(CFrame.new(model.PrimaryPart.Position + direction * speed))
                        task.wait()
                        if not _G.TpDangerSix or BijiJahe <= 10 or BijiJahe <= 25 or BijiJahe <= 50 or BijiJahe <= 70 or BijiJahe <= 101 then
                            break
                        end
                    end
                end
            end
        end)
    end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.TpDangerSix then
                local boat = workspace.Boats[_G.Dangers]
                for _, v in pairs(boat:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = false
                    end
                end
            end
        end)
    end
end)

