    spawn(function()
        while wait() do
            if _G.Connect2 then
                _G.KillGhostShip = true
            else
                _G.UseSkillPirateShip = false
            end
        end
    end)

        function CheckPirateBoatJir()
            local checkmmpb = {"PirateGrandBrigade", "PirateBrigade"}
            for r, v in next, game:GetService("Workspace").Enemies:GetChildren() do
                if table.find(checkmmpb, v.Name) and v:FindFirstChild("Health") and v.Health.Value > 0 then
                    return v
                end
            end
        end
spawn(function()
    while wait() do
        pcall(function()
            if _G.KillGhostShip then
                if CheckPirateBoatJir() then
                    game:GetService("VirtualInputManager"):SendKeyEvent(true, 32, false, game)
                    wait(0.5)
                    game:GetService("VirtualInputManager"):SendKeyEvent(false, 32, false, game)
                    local v = CheckPirateBoatJir()
                    repeat
                        wait()
                        local player = game.Players.LocalPlayer
                        local character = player.Character or player.CharacterAdded:Wait()
                        local humanoid = character:WaitForChild("Humanoid")
                        if _G.SaveAntiDie and humanoid.Health <= 4000 then
                        spawn(HyperCahaya(v.Engine.CFrame * CFrame.new(0, 120, 45), 1))
                        else
                        spawn(HyperCahaya(v.Engine.CFrame * CFrame.new(0, 50, 0), 1))
                        end
                        matquyenkiemsoat = true
                        getgenv().noclip = true
                        bjirFullPower = true
                        getgenv().psskill = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -5, 0)
                    until v.Parent or v.Health.Value <= 0 or not CheckPirateBoatJir()
                    getgenv().psskill = nil
                    bjirFullPower = false
                    matquyenkiemsoat = false
                    getgenv().noclip = false
                end
            end
        end)
    end
end)

spawn(function()
    while wait() do
        if _G.KillGhostShip then
            pcall(function()
                if CheckPirateBoatJir() then
                    local player = game.Players.LocalPlayer
                    local checkmmpb = {"PirateGrandBrigade", "PirateBrigade"}
                    
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if table.find(checkmmpb, enemy.Name) then
                            local engine = enemy:FindFirstChild("Engine")
                            if engine then
                                local distance = (player.Character.HumanoidRootPart.Position - engine.Position).Magnitude
                                AutoHaki()
                                game:GetService("VirtualUser"):CaptureController()
                                game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
                                if distance <= 65 then
                                    print("Step Kill")
									for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
										if v:IsA("Tool") then
											if v.ToolTip == "Melee" and SeaMelee then
												game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
											end
										end
									end
									_G.UseSkillPirateShip = true
									wait(0.8) 
									for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
										if v:IsA("Tool") then
											if v.ToolTip == "Blox Fruit" and SeaFruit then
												game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
											end
										end
									end
									_G.UseSkillPirateShip = true
									wait(0.8) 
									for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
										if v:IsA("Tool") then
											if v.ToolTip == "Sword" and SeaSword then
												game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
											end
										end
									end
									_G.UseSkillPirateShip = true
									wait(0.8) 
									for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
										if v:IsA("Tool") then
											if v.ToolTip == "Gun" and SeaGun then 
												game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
											end
										end
									end
									_G.UseSkillPirateShip = true
									wait(0.8) 
								else
                                _G.UseSkillPirateShip = false
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
end)

