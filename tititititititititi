    end)

    Mirragecheck = SerS:AddLabel("")
    SerS:AddToggle("Tween Mystic Island", _G.AutoMysticIsland, function(value)
        _G.AutoMysticIsland = value
        StopTween(_G.AutoMysticIsland)
    end)

    spawn(function()
        pcall(function()
            while wait() do
                if _G.AutoMysticIsland then
                    for i, v in pairs(game.Workspace.Map.MysticIsland:GetChildren()) do
                        if game:GetService("Workspace").Map:FindFirstChild("MysticIsland") then
                            HyperCahaya(CFrame.new(game.Workspace.Map.MysticIsland.BiggTree.Part.Position))
                        end
                    end
                end
            end
        end)
    end)

    SerS:AddButton("Teleport Advanced Fruit Dealer", function()
        TweenNpc()
    end)

    function TweenNpc()
        repeat
            wait()
        until game:GetService("Workspace").Map:FindFirstChild("MysticIsland")
        if game:GetService("Workspace").Map:FindFirstChild("MysticIsland") then
            AllNPCS = getnilinstances()
            for r, v in pairs(game:GetService("Workspace").NPCs:GetChildren()) do
                table.insert(AllNPCS, v)
            end
            for r, v in pairs(AllNPCS) do
                if v.Name == "Advanced Fruit Dealer" then
                    HyperCahaya(v.HumanoidRootPart.CFrame)
                end
            end
        end
    end
    
    
end

Sts:AddSeperator("Webhook")

function webStatus(Bjigur)
function SendMessageEMBED(LinkwebhookGlobal, embed)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    local data = {
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["description"] = embed.description,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = embed.footer.text
                }
            }
        }
    }
    local body = http:JSONEncode(data)
    local response = request({
        Url = LinkwebhookGlobal,
        Method = "POST",
        Headers = headers,
        Body = body
    })
end

local LinkwebhookGlobal = Bjigur


local TimeZne

local LocalizationService = game:GetService("LocalizationService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local name = player.Name

local result, code = pcall(function()
    return LocalizationService:GetCountryRegionForPlayerAsync(player)
end)

if result then

    local GameCheck = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name    
    local date = os.date("*t")
    local hour = (date.hour) % 24
    local ampm = hour < 12 and "AM" or "PM"
    local timezone = string.format("%02i:%02i:%02i %s", ((hour - 1) % 12) + 1, date.min, date.sec, ampm)
    local datetime = string.format("%02d/%02d/%04d", date.day, date.month, date.year)

    TimeZne = "" .. datetime .. " [ " .. code .. " ] : " .. timezone
    
local embed = {
    ["title"] = "Account Check Webhook",
    ["description"] = "Powered By GantengHub Premium",
    ["color"] = 15844367,
    ["fields"] = {
        {
            ["name"] = "**[👤] Name Player : **",
            ["value"] = "``` ".. game.Players.LocalPlayer.DisplayName .." ```"
        },
        {
            ["name"] = "**[📑] Status Level : **",
            ["value"] = "``` ".. game:GetService("Players").LocalPlayer.Data.Level.Value .." ```"
        },
        {
            ["name"] = "**[📑] Status Exp LvL : **",
            ["value"] = "``` ".. game:GetService("Players").LocalPlayer.Data.Exp.Value .." ```"
        },
        {
            ["name"] = "**[📑] Status Stats Player : **",
            ["value"] = "``` Demon Fruit : ".. game:GetService("Players").LocalPlayer.Data.Stats["Demon Fruit"].Level.Value .."\nDefense : ".. game:GetService("Players").LocalPlayer.Data.Stats["Defense"].Level.Value .."\nMelee : ".. game:GetService("Players").LocalPlayer.Data.Stats["Melee"].Level.Value .."\nSword : ".. game:GetService("Players").LocalPlayer.Data.Stats["Sword"].Level.Value .."\nGun : ".. game:GetService("Players").LocalPlayer.Data.Stats["Gun"].Level.Value .."```"
        },
        {
            ["name"] = "**[📑] Status Stats Point : **",
            ["value"] = "``` ".. game:GetService("Players").LocalPlayer.Data.Points.Value .." ```"
        },
        {
            ["name"] = "**[📑] Status Beli : **",
            ["value"] = "``` ".. game:GetService("Players").LocalPlayer.Data.Beli.Value .." ```"
        },
        {
            ["name"] = "**[📑] Status Spawn Point : **",
            ["value"] = "``` ".. game:GetService("Players").LocalPlayer.Data.SpawnPoint.Value .." ```"
        },
        {
            ["name"] = "**[📑] Status Game : **",
            ["value"] = "``` ".. GameCheck .." ```"
        },
        {
            ["name"] = "**[👥] Players In Server : **",
            ["value"] = "``` ".. #game:GetService("Players"):GetPlayers() .."/12 ```"
        },
        {
            ["name"] = "**[🔗] Server Job Id : **",
            ["value"] = "``` game.ReplicatedStorage['__ServerBrowser']:InvokeServer('teleport','"..game.JobId.."') ```"
        }
    },
    ["footer"] = {
        ["text"] = " | "..TimeZne.." | "
    }
}
SendMessageEMBED(LinkwebhookGlobal, embed)
end
