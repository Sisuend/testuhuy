                                        game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
                                        sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
                                    until not  _G.BlueSpikC or not v.Parent or v.Humanoid.Health <= 0
                                end
                            end
                        end
                    else
                    HyperCahaya(CFrame.new(913.2352905273438, 181.12417602539062, 33441.97265625))
                        if game:GetService("ReplicatedStorage"):FindFirstChild("Cursed Captain") then
                            HyperCahaya(game:GetService("ReplicatedStorage"):FindFirstChild("Cursed Captain").HumanoidRootPart.CFrame * CFrame.new(2,20,2))
                        else
                            if  _GHopItemSea then
                                HopServer()
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    
