    SE:AddToggle('Auto Kill Leviathan', _G.KillLevi, function(value)
    _G.KillLevi = value
    StopTween(_G.KillLevi)
    end)
    
    spawn(function()
        while wait() do
            if _G.KillLevi then
                pcall(function()                   
					for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
						if v:FindFirstChild("Leviathan Segment") and v:FindFirstChild("HumanoidRootPart") then
						    local player = game.Players.LocalPlayer
						    local distance = (player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
						    AutoHaki()
						    local player = game.Players.LocalPlayer
						    local character = player.Character or player.CharacterAdded:Wait()
						    local humanoid = character:WaitForChild("Humanoid")
						    if _G.SaveAntiDie and humanoid.Health <= 4000 then
						    HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,670,250))
						    else
							HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,450,45))
							end
							game:GetService("VirtualUser"):CaptureController()
							game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
							if distance <= 460 then
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Melee" and SeaMelee then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Blox Fruit" and SeaFruit then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Sword" and SeaSword then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Gun" and SeaGun then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
							else
							UseKillSea = false
							end
						elseif v:FindFirstChild("Leviathan Tail") and v:FindFirstChild("HumanoidRootPart") then
						    local player = game.Players.LocalPlayer
						    local distance = (player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
						    AutoHaki()
						    local player = game.Players.LocalPlayer
						    local character = player.Character or player.CharacterAdded:Wait()
						    local humanoid = character:WaitForChild("Humanoid")
						    if _G.SaveAntiDie and humanoid.Health <= 4000 then
						    HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,670,250))
						    else
							HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,450,45))
							end
							game:GetService("VirtualUser"):CaptureController()
							game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
							if distance <= 460 then
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Melee" and SeaMelee then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Blox Fruit" and SeaFruit then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Sword" and SeaSword then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Gun" and SeaGun then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								UseKillSea = true
								wait(0.8) 
							else
							UseKillSea = false
						    end
						elseif v:FindFirstChild("Leviathan") and v:FindFirstChild("HumanoidRootPart") then
						    local player = game.Players.LocalPlayer
						    local distance = (player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
						    AutoHaki()
						    local player = game.Players.LocalPlayer
						    local character = player.Character or player.CharacterAdded:Wait()
