                repeat HyperCahaya(game:GetService("Players")[_G.SelectPlyAnjay].Character.HumanoidRootPart.CFrame) wait() until _G.TeleportPlyFriend == false
            end
            StopTween(_G.TeleportPlyFriend)
        end)
    end)

