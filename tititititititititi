    _G.Set = value
end)

spawn(function()
    pcall(function()
        while wait() do
            if _G.Set then
                 local args = {
                 [1] = "SetSpawnPoint"
                 }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
        end
    end)
end)

    
    Main:AddLabel("Chest Farm")
    
Main:AddToggle("Farm Chest | safe ",false,function(value)
 AutoFarmChest = value
 StopTween(AutoFarmChest)
 end)
 
spawn(function()
    while wait() do
        pcall(function()
            if AutoFarmChest then
                local playerPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                local jarakTerpendek, chestTerdekat = math.huge, nil

                for _, child in ipairs(workspace:GetChildren()) do
                    if string.lower(child.Name):find("chest") and child:IsA("Part") then
                        local jarak = (playerPos - child.Position).magnitude
                        if jarak < jarakTerpendek then
                            jarakTerpendek, chestTerdekat = jarak, child
                        end              
                    end
                end

                if AutoFarmChest and chestTerdekat then
                    if AutoFarmChest == true then
                       local bajigurAj = chestTerdekat.CFrame
                       HyperCahaya(bajigurAj)
                       firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, chestTerdekat, 0)
                    end
                end
            end
        end)
    end
end)


