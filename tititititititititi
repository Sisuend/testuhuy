SE:AddSeperator(" Optional Feature ") 

SE:AddMultiDropdown("Select Sea Monster", {"Kill Sea Beast", "Kill PirateShips", "Kill Terrorshark", "Kill Shark", "Kill Piranha", "Kill Fish Crew Member", "Kill Haunted Crew Member", "Kill GhostShip"}, function(tbl)
    for _, option in ipairs(tbl) do
        if option == "Kill Sea Beast" then
            _G.Connect1 = true
        elseif option == "Kill PirateShips" then
            _G.Connect2 = true
        elseif option == "Kill Terrorshark" then
            _G.Connect3 = true
        elseif option == "Kill Shark" then
            _G.Connect4 = true
        elseif option == "Kill Piranha" then
            _G.Connect5 = true
        elseif option == "Kill Fish Crew Member" then
            _G.Connect6 = true
        elseif option == "Kill Haunted Crew Member" then
            _G.Connect7 = true
        elseif option == "Kill GhostShip" then
            _G.Connect8 = true
        end
    end

    if #tbl == 0 then
        print(" False All + Stoptween All ") 
        _G.Connect1 = false
        _G.Connect2 = false
        _G.Connect3 = false
        _G.Connect4 = false
        _G.Connect5 = false
        _G.Connect6 = false
        _G.Connect7 = false
        _G.Connect8 = false
                
        _G.AutoSeaBest = false
        _G.KillGhostShip = false
        _G.bjirTerrorshark = false
        _G.bjirShark = false
        _G.bjirPiranha = false
        _G.fishCrewMember = false
        _G.HountedCrew = false
        _G.bjirFishBoat = false

        StopTween(_G.AutoSeaBest)
        StopTween(_G.KillGhostShip)
        StopTween(_G.bjirTerrorshark)
        StopTween(_G.bjirShark)
        StopTween(_G.bjirPiranha)
        StopTween(_G.fishCrewMember)
        StopTween(_G.HountedCrew)
        StopTween(_G.bjirFishBoat)
    end
end)


    spawn(function()
        while wait() do
            if _G.Connect1 then
                _G.AutoSeaBest = true
            else
                _G.UseSkillSeaBeast = false
            end
        end
    end)
    
    spawn(function()
        while wait() do
            if _G.AutoSeaBest then
                pcall(function()                   
					for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
						if v:FindFirstChild("HumanoidRootPart") then
						    local player = game.Players.LocalPlayer
						    local distance = (player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
						    AutoHaki()
						    local player = game.Players.LocalPlayer
						    local character = player.Character or player.CharacterAdded:Wait()
						    local humanoid = character:WaitForChild("Humanoid")
						    if _G.SaveAntiDie and humanoid.Health <= 4000 then
						    HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,650,250))
						    else
							HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(1,450,45))
							end
							game:GetService("VirtualUser"):CaptureController()
							game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
							if distance <= 456 then
							print("Step Kill")
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Melee" and SeaMelee then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								print("Step Skill")
								_G.UseSkillSeaBeast = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Blox Fruit" and SeaFruit then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								_G.UseSkillSeaBeast = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Sword" and SeaSword then
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								_G.UseSkillSeaBeast = true
								wait(0.8) 
								for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
									if v:IsA("Tool") then
										if v.ToolTip == "Gun" and SeaGun then 
											game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
										end
									end
								end
								_G.UseSkillSeaBeast = true
								wait(0.8) 
						    end
						end
					end
					_G.UseSkillSeaBeast = false
					print("Yoss") 
                end)
            end
        end
    end)
    
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
		pcall(function()
			if _G.UseSkillSeaBeast then
			    if _G.SkillZ then
				game:GetService("VirtualInputManager"):SendKeyEvent(true,"Z",false,game)
				wait(0.1)
                game:GetService("VirtualInputManager"):SendKeyEvent(false,"Z",false,game)
