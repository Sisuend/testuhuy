                                        HyperCahaya(v.HumanoidRootPart.CFrame * CFrame.new(PosX,PosY,PosZ))
                                        game:GetService("VirtualUser"):CaptureController()
                                        game:GetService("VirtualUser"):Button1Down(Vector2.new(1280,672))
                                        sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
                                    until not  _G.PinkCoats or not v.Parent or v.Humanoid.Health <= 0
                                end
                            end
                        end
                    else
                    HyperCahaya(CFrame.new(5252.54248046875, 88.73291015625, 732.9336547851562))
                        if game:GetService("ReplicatedStorage"):FindFirstChild("Swan") then
                            HyperCahaya(game:GetService("ReplicatedStorage"):FindFirstChild("Swan").HumanoidRootPart.CFrame * CFrame.new(2,20,2))
                        else
                            if  _GHopItemSea then
                                HopServer()
                            end
                        end
                    end
                end)
            end
        end
    end)
    
