SerS:AddSeperator("Moon Check")
    
local moon_finder = SerS:AddLabel("Moon Phase : 0% ")

spawn(function()
    while wait() do
        pcall(function()
            local moonTextureId = game:GetService("Lighting").Sky.MoonTextureId

            if moonTextureId == "http://www.roblox.com/asset/?id=9709149431" then
                moon_finder:Set("Moon Phase : (🌑) : Full Moon !!")
            elseif moonTextureId == "http://www.roblox.com/asset/?id=9709149052" then
                moon_finder:Set("Moon Phase : (🌒) : 75%")
            elseif moonTextureId == "http://www.roblox.com/asset/?id=9709143733" then
                moon_finder:Set("Moon Phase : (🌓) : 50%")
            elseif moonTextureId == "http://www.roblox.com/asset/?id=9709150401" then
                moon_finder:Set("Moon Phase : (🌗) : 25%")
            elseif moonTextureId == "http://www.roblox.com/asset/?id=9709149680" then
                moon_finder:Set("Moon Phase : (🌖) : 15%")
            end
        end) 
    end
end)

SerS:AddSeperator("Haki Check")

local haki_finder = SerS:AddLabel("Haki : Not Spawn")
local haki_rank = SerS:AddLabel("Rank : Not Spawn")
local haki_type = SerS:AddLabel("Type : Not Spawn")
local haki_price = SerS:AddLabel("Price : Not Spawn") 

spawn(function()
    while wait() do
