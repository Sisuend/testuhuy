SE:AddSeperator(" Cooldown Leviathan ") 

local remote = game:GetService("ReplicatedStorage").Remotes["CommF_"]
local checkCooldown = SE:AddLabel("Cooldown Status : ??? ")
local checkSpyNpc = SE:AddLabel("Spy Npc Status : ??? ")

spawn(function()
    pcall(function()
        while wait(1.5) do
            local result = remote:InvokeServer("InfoLeviathan", "1")
            if result == -1 then
                checkCooldown:Set("Cooldown Status : ‼️ Cooldown")
                checkSpyNpc:Set("Spy Npc Status : ‼️ Cooldown") 
            elseif result == 1 then
                checkCooldown:Set("Cooldown Status : ✅ No Cooldown")
                checkSpyNpc:Set("Spy Npc Status : Bribe 1") 
            elseif result == 2 then
                checkCooldown:Set("Cooldown Status : ✅ No Cooldown")
                checkSpyNpc:Set("Spy Npc Status : Bribe 2") 
            elseif result == 3 then
                checkCooldown:Set("Cooldown Status : ✅ No Cooldown")
                checkSpyNpc:Set("Spy Npc Status : Bribe 3") 
            elseif result == 4 then
                checkCooldown:Set("Cooldown Status : ✅ No Cooldown")
                checkSpyNpc:Set("Spy Npc Status : Bribe 4") 
            elseif result == 5 then
                checkCooldown:Set("Cooldown Status : ✅ No Cooldown")
                checkSpyNpc:Set("Spy Npc Status : Leviathan Spawn Go Find !!")
            end
        end
    end)
end)

SE:AddToggle("Auto Clear Cooldown [ Buy Boat ]",_G.clearCD,function(state)
    if state then
        _G.ClearCdffr = true
    else
        _G.clearCD = false
        _G.ClearCdffr = false
        StopTween(_G.ClearCdffr) 
        StopTween(_G.clearCD)
    end

if _G.ClearCdffr then
    local teleportCompleted = false

    HyperCahaya(CFrame.new(-16921.853515625, 9.0863618850708, 433.9601135253906))

    spawn(function()
        while wait() do
            pcall(function()
                local playerPosition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                if (CFrame.new(-16921.853515625, 9.0863618850708, 433.9601135253906).Position - playerPosition).Magnitude <= 5 then
                    teleportCompleted = true
                end
            end)
        end
    end)

    repeat
        wait()
    until teleportCompleted
    _G.ClearCdffr = false
    wait()
    _G.clearCD = true
end
if _G.clearCD then
if workspace.Boats:FindFirstChild(_G.selectBoatClearCD) then
workspace.Boats[_G.selectBoatClearCD]:Destroy()
notif("System", "Some Boat Has Ben Destroy") 
wait(1)
end

wait(0.5) 

local args = {
    [1] = "BuyBoat",
    [2] = _G.selectedBoatBuy
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

wait(0.5) 

for _,v in next, workspace.Boats[_G.selectedBoatSail]:GetDescendants() do
    if v.Name:find("VehicleSeat") then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
    end
end
end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.clearCD then
                for _, v in next, workspace.Boats[_G.selectedBoatSail]:GetDescendants() do
                    if v.Name:find("VehicleSeat") then
                        wait(2) 
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                        break
                    end
                end

                local vehicleSeat = nil
                local enemyTypes = {
                    {name = "Terrorshark", variable = "bjirTerrorshark"},
                    {name = "Shark", variable = "bjirShark"},
                    {name = "Piranha", variable = "bjirPiranha"},
                    {name = "Fish Crew Member", variable = "fishCrewMember"},
                    {name = "Haunted Crew Member", variable = "HountedCrew"}, 
                    {name = "FishBoat", variable = "bjirFishBoat"}, 
                    {name = "PirateGrandBrigade", variable = "KillGhostShip"},
                    {name = "PirateBrigade", variable = "KillGhostShip"},
                }

                for _, v in next, workspace.Boats[_G.selectedBoatSail]:GetDescendants() do
                    if v.Name:find("VehicleSeat") then
                        vehicleSeat = v
                        wait(0.2) 

                        for _, enemyType in pairs(enemyTypes) do
                            local enemyName = enemyType.name
                            local enemyVariable = enemyType.variable
                            
                            if game:GetService("Workspace").Enemies:FindFirstChild(enemyName) then
                                print("Kill Enemy") 
                                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                                wait(0.1) 
                                _G[enemyVariable] = true

                                -- Menunggu musuh mati
                                while game:GetService("Workspace").Enemies:FindFirstChild(enemyName) do
                                    _G[enemyVariable] = true
                                    wait(0.2) 
                                end

                                _G[enemyVariable] = false
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = vehicleSeat.CFrame
                                
                            elseif game:GetService("Workspace").SeaBeasts:GetChildren() then
                                for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
                                    if v:FindFirstChild("HumanoidRootPart") then
                                        print("Kill Seabeast") 
                                        
                                        game.Players.LocalPlayer.Character.Humanoid.Sit = false
                                        wait(0.2) 
                                        _G.AutoSeaBest = true
                                        
                                        while wait() do
                                            if #game:GetService("Workspace").SeaBeasts:GetChildren() == 0 then
                                                print("SeaBeasts folder is empty. Proceeding...")
                                                break
                                            else
                                                print("Wait until SeaBeast Death")
                                                wait(0.2)
                                            end
                                        end

                                        _G.AutoSeaBest = false
                                        wait(0.1) 
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = vehicleSeat.CFrame
                                    end
                                end
                            elseif game.Workspace._WorldOrigin.Locations:FindFirstChild('Frozen Dimension') then
                            _G.clearCD = false
                            wait(0.5)
                            game.Players.LocalPlayer.Character.Humanoid.Sit = false
                            wait(0.5)
                            _G.BjirFrozenCuy = true
                            end
                        end
                    end
                end
            end
        end)
    end
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.clearCD then
                local batuLaut = {"SmallGroup", "SmallCluster", "MediumGroup", "MediumFlat", "Large", "Largest"}

