    local hauntedCastle = map["Haunted Castle"]
    local trophies = hauntedCastle.Trophies
    local C12 = map["Haunted Castle"]["Tablet"].Segment6.Line.Orientation
    task.wait()   
until C12 == Vector3.new(0, 0, 0)
repeat
    fireclickdetector(game:GetService("Workspace").Map["Haunted Castle"]["Tablet"].Segment7.ClickDetector)
    local workspace = game:GetService("Workspace")
    local map = workspace.Map
